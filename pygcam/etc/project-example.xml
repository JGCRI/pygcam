<?xml version="1.0" encoding="UTF-8"?>
<!--
This file defines one or more "projects" that are run using the runProjects.py script.
-->
<projects xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:noNamespaceSchemaLocation="project-schema.xsd">
	<project name="paper1">
		<vars>
			<!-- User variables, used only by defined steps -->
			<var name="startYear">2015</var>
			<var name="shockYear">2020</var>
			<var name="endYear">2050</var>
			<var name="years" eval="1">{startYear}-{endYear}</var>
			<var name="queryPath" eval="1">{GCAM.QueryDir}:{GCAM.QueryDir}/Main_queries_customized.xml</var>
		</vars>

		<steps>
			<!--
			The names defined in steps can be used on the command-line to limit operations to these steps
			If runFor="baseline", the step is run only for baseline scenarios. If runFor="policy", the step
			is run only for non-baseline scenarios. If not set (or set to any other value) the step is run
			for all scenarios. Steps with same seq number can be run in any order.
			 -->
			<!-- NOTE: the -S flag in the two 'setup' steps should not be necessary, but this requires
			     mods that must still accommodate the setup used for otaq2016. -->
			<step name="setup" runFor="baseline">@setup -b {baseline} -g {scenarioGroup} -S {scenarioSubdir} -w {scenarioDir} -p {endYear} -y {shockYear}-{endYear}</step>
			<!-- pre-query creates a batch file that can be run by GCAM when using an in-memory database -->
			<!-- <step name="prequery" runFor="baseline">@query -p -o {batchDir} -w {scenarioDir} -s {scenario} -Q "{queryPath}" "@{queryNameFile}" </step> -->
			<step name="prequery" runFor="baseline">@query -p -o {batchDir} -w {scenarioDir} -s {scenario} -Q "{queryPath}" -q "{queryXmlFile}"</step>
			<step name="gcam"  runFor="baseline">@gcam -s {baseline} -w {scenarioDir} -S {projectXmlDir} </step>
			<!-- <step name="query" runFor="baseline">@query -o {batchDir} -w {scenarioDir} -s {scenario} -Q "{queryPath}" "@{queryNameFile}"</step> -->
			<step name="query" runFor="baseline">@query -o {batchDir} -w {scenarioDir} -s {scenario} -Q "{queryPath}" -q "{queryXmlFile}"</step>

			<!-- Explicitly require policy setup, gcam, and query steps to run after baseline in case we use baseline results in constraints -->
			<step name="setup" runFor="policy">@setup -b {baseline} -s {scenario} -g {scenarioGroup} -S {scenarioSubdir} -w {scenarioDir} -p {endYear} -y {shockYear}-{endYear}</step>
			<!-- <step name="prequery" runFor="policy">@query -p -o {batchDir} -w {scenarioDir} -s {scenario} -Q "{queryPath}" "@{queryNameFile}"</step> -->
			<step name="prequery" runFor="policy">@query -p -o {batchDir} -w {scenarioDir} -s {scenario} -Q "{queryPath}" -q "{queryXmlFile}"</step>
			<step name="gcam"  runFor="policy">@gcam -S {projectXmlDir} -s {scenario} -w {scenarioDir}</step>
			<step name="query" runFor="policy">@query -o {batchDir} -w {scenarioDir} -s {scenario} -Q "{queryPath}" -q "{queryXmlFile}"</step>
			<!--
			<step name="plot"  runFor="all">@chart {scenPlotArgs} - -scenario {scenario} - -fromFile {scenPlots}</step>
			-->
			<!-- <step name="diff"  runFor="policy">@diff -D {sandboxDir} -y {years} -Y {shockYear} -q {queryNameFile} -i {baseline} {scenario}</step> -->
			<step name="diff"  runFor="policy">@diff -D {sandboxDir} -y {years} -Y {shockYear} -q {queryXmlFile} {baseline} {scenario}</step>

			<step name="plotDiff" runFor="policy">@chart {diffPlotArgs} --reference {baseline} --scenario {scenario} --fromFile {diffPlots}</step>
			<step name="xlsx" runFor="policy">@diff -D {diffsDir} -c -y {years} -Y {shockYear} -o diffs.xlsx {diffsDir}/*.csv</step>
			<step name="xlsx" runFor="policy">@diff -D {diffsDir} -c -y {years} -Y {shockYear} -o "{scenario}-annual.xlsx" -i {diffsDir}/*.csv</step>
			<step name="CI" runFor="policy">@CI -b {baseline} -p {scenario} -d {diffsDir} -f {diffsDir}/fuelEJ.txt -s "_INFER_" -y {years} -Y {shockYear}</step>

			<step name="plotCI" runFor="policy">@chart {plotCIArgs} -n3 -S -F {diffsDir}/fuelEJ.txt "Emissions-changes-{scenario}-{baseline}.csv"</step>
			<step name="plotCI" runFor="policy">@chart {plotCIArgs} --reference {baseline} --scenario {scenario} --fromFile {ciPlots}</step>
		</steps>

		<vars>
			<var name="plotCIArgs" eval="1">-D {diffsDir} -n3 -d figures -y {years} --barWidth=0.25 --box --label --legendY='-0.2' -z -r 0 --sumYears -Y 'g CO$_2$e / MJ' -T 'Contributions to carbon intensity'</var>
		</vars>

